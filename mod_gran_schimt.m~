%function [ Q ] = mod_gran_schimt( A )
%Simple algorithm for Modified Gran schimt that better than
%classical modified Gran schimt

A=[1 2 3; 2 3 4; ]
[m,n]=size(A);
q=zeros(m,n);

for i=1:n
    
c=A(:,i);

r=c;

for j=1:i-1
    
    r=r-dot(q(:,j),r)*q(:,j);
    
end

q(:,i)=r/norm(r);
    
    
end







%end




